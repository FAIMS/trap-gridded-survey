  List attribsList = new ArrayList();
  for (ref : getRefsMatching(pattern))
    attribsList.add(getAttributeName(ref));

  attribsSet  = new HashSet(attribsList);
  attribsList = new ArrayList(attribsSet);

  return attribsList;
