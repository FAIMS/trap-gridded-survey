String listToSqlString(List l){
  String sqlString = "(";
  for (int i = 0; i < l.size() - 1; i++) {
    element = l.get(i);
    sqlString += "'" + element + "', ";
  }
  if (l.size() > 0) {
    int lastIndex = l.size() - 1;
    element = l.get(lastIndex);
    sqlString += "'" + element + "'";
  }
  sqlString += ")";

  return sqlString;
}

